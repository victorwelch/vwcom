@{
    ViewData["Title"] = "VictorWelch.Com: About";
}
@section Scripts {
    <script src="~/lib/js4view/About.js"></script>
}
<div id="divBrdrContainer" style="border:1px solid transparent;position:relative;margin:0px;padding:0px;">
    <div id="divTlineBrdrContainer00" class="myBrdrContainerTopBotLine"></div>
    <div id="divBlineBrdrContainer00" class="myBrdrContainerTopBotLine"></div>
    <div id="divLlineBrdrContainer00" class="myBrdrContainerLeftRightLine"></div>
    <div id="divRlineBrdrContainer00" class="myBrdrContainerLeftRightLine"></div>
    <div id="divUlCrnrBrdrContainer00" class="myBrdrContainerCrnr myUlCrnrImage"></div>
    <div id="divUrCrnrBrdrContainer00" class="myBrdrContainerCrnr myUrCrnrImage"></div>
    <div id="divLlCrnrBrdrContainer00" class="myBrdrContainerCrnr myLlCrnrImage"></div>
    <div id="divLrCrnrBrdrContainer00" class="myBrdrContainerCrnr myLrCrnrImage"></div>
    <div id="jumbo1" class="jumbotron" style="margin:20px !important;min-height:unset;">
        <section id="secContainer">
            <div id="divInfoImg" class="infoImgClass">
            </div>
        </section>
    </div>
</div>

<div id="myRow" class="row myRow">
    <div id="col1" class="col-md-6 myColMd6Left">
        <div id="divTopLineCol1_00" class="myColTopBotLine myTopLineCol"></div>
        <div id="divBotLineCol1_00" class="myColTopBotLine myBotLineCol"></div>
        <div id="divLeftLineCol1_00" class="myColLeftRightLine myLeftLineCol"></div>
        <div id="divRightLineCol1_00" class="myColLeftRightLine myRightLineCol"></div>
        <div id="divUlCol1_00" class="myColCorner myUlCrnrImage myUlColCorner"></div>
        <div id="divUrCol1_00" class="myColCorner myUrCrnrImage myUrColCorner"></div>
        <div id="divLlCol1_00" class="myColCorner myLlCrnrImage myLlColCorner"></div>
        <div id="divLrCol1_00" class="myColCorner myLrCrnrImage myLrColCorner"></div>
        <div id="divCol1Wrap" style="clear:both;position:relative;top:0;left:0;">
            <div>
                <h2 style="margin-left:25px">About Info:</h2>
                <div class="myColTextDiv">
                    <p>
                        This is a developmental web-site for trying out different techniques for web development, particulary with regard to user interfaces.
                    </p>
                    <p>
                        Further, this web site was built using <b><i>ASP.NET Core/C#/MVC 5.0</i></b> in combination with <b><i>Angular 8</i></b>.
                    </p>
                    <p>
                        The highly under-utilized back end is comprised of both <b><i>Sql Server</i></b> and <b><i>MySql</i></b> data base management systems.
                    </p>
                    <p>
                        Also, the site makes heavy use of the <b><i>Bootstrap</i></b> toolkit along with <b><i>JQuery</i></b> and custom <b><i>JQuery plugins</i></b>.
                    </p>
                </div>
                <div id="divExtra1" style="clear:both;position:relative;top:0;left:0;background-color:transparent;z-index:1;height:50px;">
                    <img id="imgExtra1" src="~/Images/spacer.gif" height="1" width="1" style="position:absolute;top:0;left:0;z-index:1;height:50px;" />
                </div>
            </div>
        </div>
        <div id="spacerdiv1" style="clear:both;position:relative;top:0;left:0;background-color:transparent;z-index:1;display:none;">
            <img id="spacerimg1" src="~/Images/spacer.gif" height="1" width="1" style="position:absolute;top:0;left:0;z-index:1;display:none;" />
        </div>
    </div>
    <div id="col2" class="col-md-6 myColMd6Right">
        <div id="divTopLineCol2_00" class="myColTopBotLine myTopLineCol"></div>
